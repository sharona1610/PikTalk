  <title>Login</title>
</head>
<body>

  <nav>
    <div class="nav-wrapper">
      <a href="/auth/signup" class="right">Signup</a>
      <h3>PikTalk</h3>
    </div>
  </nav>
  <div class="contain">
    <form class='form-group signup' action="/auth/login" method="POST">
      <div class=''>
        <label class="label" for="authEmail">Email</label>
        <input id="authEmail" class="" type="email" name="email">

        <label class="label" for="authPassword">Password</label>
        <input id="authPassword" class="" type="password" name="password">


        <input class="btn" type="submit">
      </div>
    </form>
  </div>

  <script type="text/javascript" src="https://code.jquery.com/jquery-2.1.1.min.js"></script>
  <script src="https://cdnjs.cloudflare.com/ajax/libs/materialize/0.97.8/js/materialize.min.js"></script>

</body>
